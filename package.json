    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.79.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.8",
