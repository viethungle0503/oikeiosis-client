    "mitt": "^3.0.1",
    "pinia": "^2.2.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.7",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.0.7",
    "@rushstack/eslint-patch": "^1.10.4",
